{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },

    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "comma-spacing":"off",
        "no-trailing-spaces": "off",
        "indent": "off",
        "linebreak-style":"off",
       "space-before-blocks":"off",
        "quotes":"off",
        "key-spacing": "off",
        "object-curly-spacing":"off",
        "eol-last":"off",
        "max-len":"off",
        "require-jsdoc":"off",
        "padded-blocks":"off",
        "no-undef":"off",
        "spaced-comment":"off"
    },

    "extends":["eslint:recommended", "google"]
}